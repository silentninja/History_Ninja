{
  "manifest_version": 2,
  "name": "History Ninja",
  "version": "1.0",
  "description": "Stores your browsing history to your dropbox account",
"options_page": "options.html",
 "browser_action": {
          "default_title": "History Ninja"
                  },
        "background": {
        "scripts": ["background.js","jquery.min.js"]
    },
"update_url": "https://github.com/silentninja/History_Ninja/updates.xml",
"web_accessible_resources": [ "option.js","jquery.min.js" ],
    "permissions": [
    "tabs"
]
}